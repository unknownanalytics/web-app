<p id="notice"><%= notice %></p>

<h1>New Api key for  <%= current_domain.name %> </h1>

<%= render 'form', api_key: @api_key %>

<h1><%= current_domain.name %> Api Keys</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @api_keys.each do |api_key| %>
    <tr>
      <td><%= api_key.alias %></td>
      <td><%= api_key.public_key %></td>
      <td><%= link_to 'Show', dashboard_domain_api_key_path(api_key.domain, api_key) %></td>
      <td><%= link_to 'Destroy', dashboard_domain_api_key_path(api_key.domain, api_key), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

