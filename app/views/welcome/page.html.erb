<% content_for :custom_css_assets do %>
  <%= stylesheet_link_tag 'welcome', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>

<% cache('shared/header') do %>
  <%= render 'shared/header' %>
<% end %>

<% cache(@page) do %>
  <%= render @page %>
<% end %>


<% content_for :custom_js_assets do %>
  <%= javascript_include_tag 'welcome', 'data-turbolinks-track': 'reload' %>
<% end %>

<% cache("welcome/home/footer") do %>
  <%= render "welcome/home/footer" %>
<% end %>

